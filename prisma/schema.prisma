generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:rootpassword@localhost:3310/mydb"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  name      String
  address   String
  phone     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
